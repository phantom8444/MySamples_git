<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>rpcServlet</servlet-name>
    <servlet-class>org.gonevertical.demo.server.RpcServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>rpcServlet</servlet-name>
    <url-pattern>/demogwtgdataoauth/rpcService</url-pattern>
  </servlet-mapping>
  
 
  <servlet>
    <servlet-name>AskForAccess</servlet-name>
    <servlet-class>org.gonevertical.demo.server.servlet.Servlet_AskForAccess</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AskForAccess</servlet-name>
    <url-pattern>/askforaccess</url-pattern>
  </servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>DemoGWTGDataOAuth.html</welcome-file>
  </welcome-file-list>

</web-app>
